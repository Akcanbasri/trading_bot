# Binance API Credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here
BINANCE_TESTNET=true

# Bot Mode (live, test, paper)
BOT_MODE=test

# Risk Management Settings
MAX_OPEN_POSITIONS=2
MAX_POSITION_SIZE_USD=50.0
MAX_POSITION_SIZE_PERCENTAGE=3.0
MAX_DAILY_LOSS_PERCENTAGE=3.0
MAX_TOTAL_LOSS_PERCENTAGE=15.0
DEFAULT_STOP_LOSS_PERCENTAGE=1.5
DEFAULT_TAKE_PROFIT_PERCENTAGE=3.0
DEFAULT_LEVERAGE=1
RISK_REWARD_RATIO=2.0
ENABLE_TRAILING_STOP=true
TRAILING_STOP_ACTIVATION_PERCENTAGE=0.8
TRAILING_STOP_CALLBACK_PERCENTAGE=0.3

# Trading Strategy Settings
STRATEGY_NAME=moving_average_crossover
STRATEGY_SHORT_PERIOD=10
STRATEGY_LONG_PERIOD=30
STRATEGY_SIGNAL_THRESHOLD=0.3
TIMEFRAME=15m
SYMBOLS=DOGEUSDT

# FiboBULL Strategy Settings (if used)
FIBOBUILL_LEFT_BARS=5
FIBOBUILL_RIGHT_BARS=5
FIBOBUILL_USE_CONFIRMATIONS=true

# Position Management
MAX_TRADES_PER_DAY=15
AUTO_CLOSE_POSITIONS_EOD=true

# Telegram Notifications
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
ENABLE_TELEGRAM_NOTIFICATIONS=true

# Logging
LOG_LEVEL=INFO
LOG_TO_FILE=true
LOG_FILE_PATH=logs/trading_bot.log 